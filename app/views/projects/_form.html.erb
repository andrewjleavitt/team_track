<%= form_with model: @project do |f| %>
  <p>
    <%= label_tag :name %><br>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= label_tag :team %><br>
    <%= f.collection_select(:team_id, @teams, :id, :name) %>
  </p>
  <p>
    <%= label_tag "Project Category" %><br>
    <%= f.collection_select(:project_category_id, @project_categories, :id, :name) %>
  </p>
  <p>
    <%= label_tag "Start Date" %><br>
    <%= f.text_field :start_at %>
  </p>
  <p>
    <%= label_tag "Due Date" %><br>
    <%= f.text_field :due_at %>
  </p>
  <p>
    <%= label_tag "Completed Date" %><br>
    <%= f.text_field :complete_at %>
  </p>
  <p>
    <%= label_tag "Status" %><br>
    <%= f.select :status, options_for_select(Project.status_options, @project.status), include_blank: true %>
  </p>
  <p>
    <%= label_tag "Cost" %><br>
    <%= f.text_field :cost %>
  </p>
  <p>
    <%= f.submit "Save" %>
  </p>
<% end %>
